{
  "manifest_version": 2,

  "name": "TAKER TWITTER",
  "description": "This extension changes the background color",
  "version": "1.0",
  "background": {
    "scripts": ["function.js"],
	"persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
	 "default_title" : "COLOR"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}